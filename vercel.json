{
  "version": 2,
  "builds": [
    {
      "src": "streamlit_app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "streamlit_app.py"
    }
  ],
  "env": {
    "PYTHONPATH": "."
  },
  "functions": {
    "streamlit_app.py": {
      "runtime": "python3.9"
    }
  }
}